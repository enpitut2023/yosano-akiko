{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "compilerOptions": {
    "downlevelIteration": true,
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "lib": ["ESNext"],
    "module": "Node16",
    "moduleResolution": "node16",
    "skipLibCheck": true,
    "strict": true,
    "target": "es2020",
    "noEmit": true
  }
}
